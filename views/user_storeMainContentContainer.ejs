<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <title></title>
  <style>
    html,
    form,
    table,
    tbody,
    body,
    span,
    iframe {
      height: 100%;
      width: 100%;
      margin: 0;
      padding: 0;
    }

    td,
    h1,
    h4 {
      margin: 0;
      padding: 0;
    }

    img {
      width: 100%;
      height: auto;
    }
  </style>
</head>

<body>
  <table>
    <%for (var i = 0; i < contents.length; i += 3) {%>
      <tr>
      <%if (contents.length - i <= 3) {
        for (var j = 0; j < contents.length - i; j++) {%>
        <td>
          <img src = "<%=contents[i + j].url%>" alt = "<%=contents[i + j].content%>" onclick = "location.href = '$<%=storeMainContentDetailUrl%> + '/' + <%=owner_auth%> + '/' + <%=contents[i + j].number%>'" >
        </td>
        <%}
      } else {
        for (var j = 0; j < 3; j++) {%>
          <td>
            <img src = "<%=contents[i + j].url%>" alt = "<%=contents[i + j].content%>" onclick = "location.href = '$<%=storeMainContentDetailUrl%> + '/' + <%=contents[i + j].number%>'">
          </td>
        <%}
      }%>
      </tr>
    <%}%>
  </table>
</body>

</html>
